    "analyze": "cross-env ANALYZE=true next build",
    "prepare": "husky install",
    "test": "jest",
    "json-server": "json-server --watch scripts/json-server/db.json --port 3002 --routes scripts/json-server/routes.json --middlewares scripts/json-server/middleware.js "
